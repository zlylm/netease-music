.scroll-view-box{
	width: 100%;
	height: calc(100vh - 70px);
	overflow: hidden;
}
.scroll-view-box scroll-view{
	height: 100%;
}
.fixbg{
	width: 100%;
	height: 100vh;
	position: fixed;
	top: 0;
	left: 0;
	/* background-image: url('./static/logo.png'); */
	background-size: cover;
	background-position: 0 center;
	-webkit-filter: blur(20px);
	        filter: blur(20px);
	-webkit-transform: scale(1.2);
	        transform: scale(1.2);
}

